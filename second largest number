import java.util.Arrays;
import java.io.*;
class Main{
  static int secondLargest(int[] arr){
    int n = arr.length;
    Arrays.sort(arr);
    for(int i=n-2;i>=0;i--){
      if(arr[i] != arr[n-1]){
        return arr[i];
      }
    }
    return -1;
  }
  public static void main (String[] args) {
    int[] arr = {23,68,20,10};
    System.out.println(secondLargest(arr));
  }
}
